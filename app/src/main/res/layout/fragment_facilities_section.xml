                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tv_electric"
                    style="@style/food_section_heading_txt_view"
                    android:text="@string/electricity"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"/>

                <LinearLayout
                    style="@style/linear_layout"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@+id/tv_electric"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent">
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tv_power_cut"
                        style="@style/food_section_txt_view"
                        android:text="@string/power_cut"
                        android:layout_marginStart="0dp"
