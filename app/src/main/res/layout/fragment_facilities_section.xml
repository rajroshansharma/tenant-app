                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tv_bill"
                        style="@style/food_section_txt_view"
                        android:text="@string/bill"
                        android:background="@drawable/shape"/>
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tv_meter"
                        style="@style/food_section_txt_view"
                        android:text="@string/meter"
                        android:background="@drawable/shape"/>
                    <com.google.android.material.textview.MaterialTextView
                        android:id="@+id/tv_inverter"
                        style="@style/food_section_txt_view"
                        android:text="@string/inverter"
                        android:background="@drawable/shape"/>
                </LinearLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/noise_mCard_view5"
            style="@style/material_card_view"
            android:layout_height="90dp"
            app:cardElevation="2dp"
            android:layout_marginTop="16dp"
            android:outlineAmbientShadowColor="@color/light_shade_gray_color"
            app:cardBackgroundColor="@color/light_shade_gray_color">


            <androidx.constraintlayout.widget.ConstraintLayout
                style="@style/constraint_layout">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/tv_noise"
                    style="@style/food_section_heading_txt_view"
                    android:text="@string/noise"
                    app:layout_constraintTop_toTopOf="parent"
